#include "stdafx.h"
#include "stdio.h"
#include "conio.h"
#include "math.h"

void main(void)
{
  int numero,i;
	


printf("determina digitos introducidos \n");
	printf("Introduzca un numero entero: \n");
	scanf("%d",&numero);

i=0;
	while(numero>0)
	{
		
		numero=(numero/10);
		i=i+1;
	}
	printf("numeros de digitos introducidos %d\n",i);
	
	getch();
}
