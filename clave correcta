#include "stdafx.h"
#include "stdio.h"
#include "conio.h"


void main(void)
{
  int clave;
	int i;

printf("Inteno de clave \n");

i=1;
do{

	printf("Introduzca clave: \n");
	scanf("%d",&clave);

if(clave!=1419914)
{
	i=i+1;
	i=i;
}

else
{
	printf("clave correcta\n");
i=4;

}	

}while((i<=3) );		
	getch();
}
