#include "stdafx.h"
#include "stdio.h"
#include "conio.h"


void main()
{
  int edad;
	printf("introduzca la edad del nadador:");
	scanf("%d", &edad);
	if(edad>5 && edad<7)
	{
		printf("EL NADADOR ES INFANTIL A");
	}
	else
	{
		if(edad>7 && edad<10)
		{printf("EL NADADOR ES INFANTIL B");
		}
		else
		{
			if(edad>10 && edad<14)
			{printf("EL NADADOR ES JUVENIL A");
			}
			else
			{
				if(edad>13 && edad<18)
				{printf("EL NADADOR ES JUVENIL B");
				}
				else
				{
					if(edad>17 && edad<61)
					{printf("EL NADADOR ES ADULTO");
					}
					else
					{printf("EL NADADOR ES SEÃ‘OR");
					}
				}
			}
		}
	}
	getch();

}
