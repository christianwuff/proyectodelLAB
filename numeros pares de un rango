#include "stdafx.h"
#include "stdio.h"
#include "conio.h"
#include "math.h"

void main(void)
{
  int comienzo,fin,suma;
	int i;

printf("Introducir cominezo\n");
scanf("%d",&comienzo);
printf("Introducir fin\n");
scanf("%d",&fin);


i=comienzo;
suma=0;

while((i<=fin) )
{
	printf("%d\n",i);
suma=suma+i;
	i=i+2;

}

printf("la suma es:%d \n",suma);
		
	getch();
}
